{
  "extends": [
    "group:babelMonorepo",
    "group:commitlintMonorepo",
    "group:docusaurusMonorepo",
    "monorepo:jest",
    "packages:eslint",
    "group:angular-eslintMonorepo",
    ":prHourlyLimit2",
    "helpers:pinGitHubActionDigests",
    ":semanticCommitTypeAll(build)",
    "npm:unpublishSafe",
    "workarounds:all"
  ],
  "timezone": "UTC",
  "rangeStrategy": "bump",
  "separateMajorMinor": true,
  "prConcurrentLimit": 2,
  "dependencyDashboard": false,
  "postUpdateOptions": ["yarnDedupeHighest"],
  "internalChecksFilter": "strict",
  "ignoreDeps": [
    "@mdx-js/react",
    "@types/react",
    "execa",
    "chalk",
    "react",
    "react-dom"
  ],
  "packageRules": [
    {
      "matchPaths": ["examples/**/package.json"],
      "matchUpdateTypes": ["patch", "minor"]
    },
    {
      "matchPackagePatterns": ["^angular", "^@angular", "^ng-packagr"],
      "groupName": "Angular packages"
    },
    {
      "matchFiles": ["**/package.json"],
      "matchDepTypes": ["dependencies", "optionalDependencies"],
      "rangeStrategy": "in-range-only"
    },
    {
      "matchDepTypes": ["peerDependencies"],
      "enabled": false
    },
    {
      "matchPackagePatterns": [
        "esbuild",
        "jsdom",
        "typescript"
      ],
      "enabled": false
    },
    {
      "matchPaths": ["examples/**/package.json"],
      "matchPackagePatterns": ["^angular", "^@angular", "ng-packagr"],
      "matchUpdateTypes": ["major"],
      "enabled": false
    },
    {
      "matchPaths": ["examples/**/package.json"],
      "matchDepNames": ["angular-in-memory-web-api", "zone.js"],
      "enabled": false
    }
  ]
}
