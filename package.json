{
  "name": "jest-preset-angular",
  "version": "9.0.0-next.9",
  "description": "Jest preset configuration for Angular projects",
  "license": "MIT",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thymikee/jest-preset-angular.git"
  },
  "bugs": {
    "url": "https://github.com/thymikee/jest-preset-angular/issues"
  },
  "homepage": "https://thymikee.github.io/jest-preset-angular",
  "author": "Michał Pierzchała <thymikee@gmail.com>",
  "contributors": [
    "Ahn <anhpnnd@gmail.com> (https://github.com/ahnpnl)",
    "Thomas <thomas@wtho.de> (https://github.com/wtho)"
  ],
  "keywords": [
    "jest",
    "typescript",
    "angular",
    "testing"
  ],
  "scripts": {
    "prebuild": "node scripts/clean.js",
    "prepare": "yarn build",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "lint:prettier": "prettier '**/*.{yml,yaml}' 'website/**/*.{css,js,md}' 'README.md'",
    "lint:prettier-format": "yarn lint:prettier --write --ignore-path .gitignore",
    "lint:prettier-ci": "yarn lint:prettier --check --ignore-path .gitignore",
    "test:unit": "yarn test-cjs:unit && yarn test-esm:unit",
    "test-cjs:unit": "jest -i",
    "test-esm:unit": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js -i -c=jest-esm.config.js",
    "test:e2e": "node scripts/e2e.js",
    "doc": "cd website && yarn start",
    "doc:build": "cd website && yarn build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "jest-environment-jsdom": "27.0.0-next.3",
    "pretty-format": "27.0.0-next.3",
    "ts-jest": "27.0.0-next.7"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": ">=0.901.12",
    "@angular/core": ">=9.0.0",
    "jest": "27.0.0-next.3"
  },
  "optionalDependencies": {
    "webpack": "5.24.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.0",
    "@angular-eslint/eslint-plugin": "latest",
    "@angular-eslint/eslint-plugin-template": "latest",
    "@angular-eslint/template-parser": "latest",
    "@angular/animations": "^11.1.0",
    "@angular/common": "^11.1.0",
    "@angular/compiler": "^11.1.0",
    "@angular/compiler-cli": "^11.1.0",
    "@angular/core": "^11.1.0",
    "@angular/platform-browser": "^11.1.0",
    "@commitlint/cli": "12.x",
    "@commitlint/config-angular": "12.x",
    "@jest/globals": "27.0.0-next.3",
    "@jest/transform": "27.0.0-next.3",
    "@jest/types": "27.0.0-next.3",
    "@types/jest": "26.x",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "conventional-changelog-cli": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "^24.1.9",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-prettier": "latest",
    "execa": "latest",
    "glob": "latest",
    "husky": "latest",
    "jest": "^27.0.0-next.3",
    "lint-staged": "latest",
    "pinst": "^2.1.4",
    "prettier": "2.x",
    "rxjs": "6.x",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "zone.js": "^0.11.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 10"
  }
}
