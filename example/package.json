    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "core-js": "^2.5.5",
    "rxjs": "6.1.0",
    "rxjs-compat": "^6.1.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.0",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.3.0",
    "jest": "^22.4.3",
    "jest-preset-angular": "^5.2.2",
    "protractor": "~5.1.2",
    "ts-node": "^6.0.2",
    "tslint": "^5.10.0",
    "typescript": "2.7.2"
  },
  "jest": {
